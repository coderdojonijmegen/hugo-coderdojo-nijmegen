		&lt;meta name="viewport" content="width=device-width, initial-scale=1"&gt;
		&lt;link rel="stylesheet" type="text/css" href="style.css" media="screen"/&gt;
		&lt;link rel="preconnect" href="https://fonts.googleapis.com"&gt;
		&lt;link rel="preconnect" href="https://fonts.gstatic.com" crossorigin&gt;
		&lt;link href="https://fonts.googleapis.com/css2?family=Cookie&amp;display=swap" rel="stylesheet"&gt;
	&lt;/head&gt;
	&lt;body&gt;

		&lt;header&gt;
			&lt;h2&gt;Ontbijtkoek Clicker (zet jouw titel hier!)&lt;/h2&gt;
		&lt;/header&gt;

		&lt;section&gt;
			&lt;div id="klikRuimte"&gt;
				&lt;img src="https://www.pngall.com/wp-content/uploads/2016/07/Cookie-PNG.png" onClick="onClickCookie()"&gt;
				&lt;br&gt;
				&lt;!-- In deze h2 tags laten we de huidige hoeveelheid geld zien, 
					en hoeveel geld we per seconde verdienen. --&gt; 
					&lt;h2 id="geld"&gt;Geld: 0$&lt;/h2&gt;
					&lt;h2 id="geldPerSeconde"&gt;Geld per seconde: 0$&lt;/h2&gt;
			&lt;/div&gt;

			&lt;!-- In deze div laten we van elk type clicker zien hoeveel we er hebben. --&gt; 
			&lt;div class="tierdisplay rows" id="actieveClickers"&gt;
				&lt;h3&gt;Actieve Clickers&lt;/h3&gt;

				&lt;div class="tier0"&gt;
					&lt;!-- Per clicker van dit type voegen we een afbeelding toe. --&gt; 
				&lt;/div&gt;

				&lt;div class="tier1"&gt;&lt;/div&gt;
				&lt;div class="tier2"&gt;&lt;/div&gt;
				&lt;div class="tier3"&gt;&lt;/div&gt;
				&lt;div class="tier4"&gt;&lt;/div&gt;
			&lt;/div&gt;

			&lt;!-- In deze div kan de speler de verschillende clickers kopen. --&gt; 
			&lt;div class="tierdisplay rows" id="clickerWinkel"&gt;
				&lt;h3&gt;Koop Clickers&lt;/h3&gt;

				&lt;!-- Via deze div kan je de Chef Clicker kopen. Deze kan je als voorbeeld gebruiken. --&gt; 
				&lt;div class="tier0 ruimte" onClick="koopClicker(0)"&gt;
					&lt;img class="icon" src="https://pngimg.com/uploads/chef/chef_PNG54.png"&gt;
					&lt;text&gt;Huur een extra chef!&lt;/text&gt;
					&lt;!-- De prijs is nu nog 1$. Deze kan je laten stijgen per aankoop! --&gt; 
					&lt;text&gt;1$&lt;/text&gt;
				&lt;/div&gt; 

				&lt;div class="tier1 ruimte" onClick="koopClicker(1)"&gt;
					&lt;img class="icon" src="???"&gt;
					&lt;text&gt;???&lt;/text&gt;
					&lt;text&gt;??? $&lt;/text&gt;
				&lt;/div&gt; 
				&lt;div class="tier2 ruimte" onClick="koopClicker(2)"&gt;
					&lt;img class="icon" src="???"&gt;
					&lt;text&gt;???&lt;/text&gt;
					&lt;text&gt;??? $&lt;/text&gt;
				&lt;/div&gt; 
				&lt;div class="tier3 ruimte" onClick="koopClicker(3)"&gt;
					&lt;img class="icon" src="???"&gt;
					&lt;text&gt;???&lt;/text&gt;
					&lt;text&gt;??? $&lt;/text&gt;
				&lt;/div&gt; 
				&lt;div class="tier4 ruimte" onClick="koopClicker(4)"&gt;
